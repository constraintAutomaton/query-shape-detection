{
  "name": "query-shape-detection",
  "version": "0.0.8",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\-test.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "lint": "eslint . --ext .ts --cache",
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@shexjs/parser": "1.0.0-alpha.26",
    "@shexjs/visitor": "1.0.0-alpha.25",
    "@types/shexj": "^2.1.4",
    "rdf-data-factory": "^1.1.2",
    "sparqlalgebrajs": "^4.3.0"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@rubensworks/eslint-config": "^2.0.0",
    "@types/jest": "^29.5.5",
    "@types/n3": "^1.16.4",
    "@types/streamify-array": "^1.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "n3": "^1.17.2",
    "streamify-array": "^1.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
